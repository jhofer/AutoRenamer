<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
   

    <add key="sourcePath" value="C:\test"/>
    <add key="moviesPath" value="C:\test\MOVIES"/>
    <add key="seriesPath" value="C:\SERIES"/>
    <add key="archivePath" value="C:\ARCHIVE"/>
    <add key="filebotInstallDir" value="C:\Program Files\FileBot\"/>
    <add key="extensions" value="*.mkv,*.avi,*.mp4"/>
    <add key="sleep" value="10000"/>
    <add key="seriesFormat" value="{n}/Season {s}/{n} - {s00e00} - {t}"/>
    <add key="moviesFormat" value="{n} ({y})/{n} ({y})"/>
    <add key="moviesDB" value="TheMovieDB"/>
    <add key="seriesDB" value="TheTVDB"/>
    <add key="moviesLang" value="de"/>
    <add key="seriesLang" value="en"/>
    <add key="seriesIdentifierRegex" value=".*S[0-9][0-9]E[0-9][0-9].*"/>
    


  </appSettings>

</configuration>